dataset=read.csv("~/Desktop/Machine Learning/drive-download-20210217T191305Z-001/Part 2 - Regression/Section 5 - Multiple Linear Regression/R/50_Startups.csv")
x=unique(dataset$State, incomparables = FALSE)
is.vector(x)
dataset$State=factor(dataset$State,levels = x,labels = c(seq(1:length(x))))
library(caTools)
set.seed(123)
split=sample.split(dataset$Profit,SplitRatio = 0.8)
training_set=subset(dataset,split==TRUE)
test_set=subset(dataset,split==FALSE)
names(dataset)
regressor=lm(Profit~R.D.Spend+Administration+Marketing.Spend+State,data=training_set)
summary(regressor)
y_pred=predict(regressor,newdata = test_set)
y_pred
